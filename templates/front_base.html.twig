        <link href="{{ asset('css/custom.css') }}" rel="stylesheet"/>
        <link href="{{ asset('css/front.css') }}" rel="stylesheet"/>
        <link href="{{ asset('css/filters.css') }}" rel="stylesheet"/>
    {% endblock %}
</head>
<body>

{% block nav %}
    {{ include('partials/_front_nav.html.twig') }}
{% endblock %}

{% block body %}{% endblock %}

